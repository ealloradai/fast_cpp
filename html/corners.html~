  var so = new SWFObject('/local/fastcpp/player.swf','mpl','470','290','9');
  so.addParam('allowfullscreen','true');
  so.addParam('allowscriptaccess','always');
  so.addParam('wmode','opaque');
  so.addVariable('file','rtsp.stream');
  so.addVariable('streamer','rtsp://150.217.35.115:554/axis-media');
  so.write('mediaspace');
